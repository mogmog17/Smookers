<div class="container">
  <div class="row">
    <h2 class="mt-4 mb-4">
      <i class="fas fa-users"></i>
      ユーザー一覧
    </h2>
    <%= render "users", users: @users %>
    <div class="d-flex justify-content-center"><%= paginate @users %></div>
  </div>
</div>
<span id="back">
  <%= link_to "#" do %>
    <%= image_tag asset_path("arrow.png"), class: "arrow" %>
  <% end %>
</span>