<div class="container">
  <div class="row">
    <h1 class="mt-4 mb-4">いいね一覧</h1>
    <table class="table table-hover table-inverse">
      <tbody>
        <% @favorite_posts.each do |f|%>
          <tr>
            <td><%= image_tag f.get_spot_image(100, 100) %></td>
            <td><%= link_to f.title, f %></td>
            <td><%= f.body %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>